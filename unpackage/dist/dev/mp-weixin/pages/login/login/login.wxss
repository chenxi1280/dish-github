page {
  background-color: #F08080;
}
.login .top {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  height: 500rpx;
  width: 750rpx;
}
.login .top .login_redirect_btn {
  color: #FCFCFC;
  font-size: 50rpx;
  font-weight: bold;
  margin-left: 100rpx;
  margin-top: 100rpx;
}
.login .top .signin_redirect_btn {
  color: #FCFCFC;
  font-weight: bold;
  margin: 20rpx;
}
.login .bottom .phoneBox {
  border: 2rpx solid black;
  height: 92rpx;
  border-radius: 10rpx;
  width: 670rpx;
  margin: 0 auto;
  margin-top: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
}
.login .bottom .phoneBox .acount {
  width: 100%;
  height: 100%;
}
.login .bottom .phoneBox .phoneIcon {
  background: url(data:image/png;base64,) no-repeat center;
  width: 80rpx;
  height: 92rpx;
  background-size: 60rpx;
}
.login .bottom .pwdBox {
  width: 670rpx;
  height: 92rpx;
  border: 2rpx solid black;
  border-radius: 10rpx;
  margin: 0 auto;
  margin-top: 50rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
}
.login .bottom .pwdBox .pwdIcon {
  background: url(data:image/png;base64,) no-repeat center;
  width: 80rpx;
  height: 80rpx;
  background-size: 60rpx;
}
.login .bottom .pwdBox .pwd {
  width: 100%;
  height: 100%;
}
.login .bottom .codeBox {
  width: 670rpx;
  height: 92rpx;
  border-radius: 10rpx;
  margin: 0 auto;
  margin-top: 50rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.login .bottom .codeBox .code {
  border: 2rpx solid black;
  border-radius: 10rpx;
  height: 100%;
  width: 60%;
  padding-left: 40rpx;
}
.login .bottom .codeBox .code input {
  width: 100%;
  height: 100%;
}
.login .bottom .codeBox .codeImage {
  border: 2rpx solid black;
  border-radius: 10rpx;
  height: 100%;
  width: 30%;
}
.login .bottom .codeBox .codeImage ._img {
  border-radius: 10rpx;
  width: 100%;
  height: 100%;
}
.login .bottom .login {
  border: 2rpx solid black;
  width: 670rpx;
  margin: 0 auto;
  margin-top: 50rpx;
}
.login .bottom .login:hover {
  background-color: #EEEEE0;
}

