.mine .top {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  width: 670rpx;
  margin: 0 auto;
  margin-top: 20rpx;
}
.mine .top .avatarUrl {
  border: 2rpx solid black;
  with: 200rpx;
  height: 250rpx;
}
.mine .top .avatarUrl image {
  width: 250rpx;
  height: 100%;
}
.mine .top .userinfo {
  /* border: 2rpx solid black; */
  with: 250rpx;
  height: 150rpx;
  margin-left: 20rpx;
}
.mine .top .userinfo input {
  margin-top: 50rpx;
  margin-left: 10rpx;
}
.mine .top .userinfo .line {
  border: 2rpx solid #D1D1D1;
  margin-left: 10rpx;
}
.mine .top .problem {
  /* border: 2rpx solid black; */
  height: 50rpx;
  width: 50rpx;
  margin-left: 20rpx;
}
.mine .top .problem .headset {
  background: url(data:image/png;base64,) center no-repeat;
  width: 100%;
  height: 100%;
  background-size: 50rpx;
}
.mine .mid {
  margin-top: 40rpx;
}
.mine .mid .loginBtn {
  text-align: center;
}
.mine .mid .line {
  margin-top: 20rpx;
  border: 2rpx solid #D1D1D1;
}
.mine .bottom .major_title {
  margin-top: 20rpx;
}
.mine .bottom .type_verify {
  margin-top: 20rpx;
}
.mine .bottom .type_publish {
  margin-top: 500rpx;
