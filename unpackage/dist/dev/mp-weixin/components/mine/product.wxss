.product {
  height: 100%;
  width: 100%;
}
.product .tips {
  border: 2rpx solid black;
  width: 160rpx;
  height: 210rpx;
  position: absolute;
  top: 0;
  left: 242rpx;
}
.product .tips .work_publish {
  width: 100%;
  height: 70rpx;
  box-sizing: border-box;
  position: relative;
}
.product .tips .work_publish .btn_publish {
  line-height: 70rpx;
  text-align: center;
  font-size: 20rpx;
}
.product .tips .work_publish .btn_publish:hover {
  background: #D3D3D3;
}
.product .tips .work_publish .line {
